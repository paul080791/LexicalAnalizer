program CircleArea;
	{Illega Operand type for NOT operator}
var
	r, a, p, b : real; 
	i, j : integer;	
begin
	r := 8;
	p := 0;
	a := 0;
	j := not p;
	if r > 5 then
	  a := (3.14) * r * r;
	if (a < 100) then
	    p := 2 * 3.14 * b; 
	{Display the results}
	writeln ( 'The output results are ' , r, p, b);
end.  
